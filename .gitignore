# From repo root
pbpaste > .gitignore  # (If you just copied the block)
# or use your editor to paste it in .gitignore

# Make sure previously tracked junk is unstaged/removed from tracking:
git rm -r --cached seventh-horizon-ui/playwright-report seventh-horizon-ui/test-results || true
git rm -r --cached **/__pycache__ || true

git add .gitignore
git commit -m "chore(gitignore): align for UI (Playwright) + Python tools"
git push
